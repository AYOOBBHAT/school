{
  "name": "mobile",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.22.0",
  "main": "index.js",
  "scripts": {
    "dev": "expo start -c --tunnel --non-interactive",
    "build": "echo 'Expo builds are handled via EAS. Use: pnpm build:android or pnpm build:ios'",
    "build:android": "pnpm eas build --platform android --profile preview",
    "build:android:prod": "pnpm eas build --platform android --profile production",
    "build:ios": "pnpm eas build --platform ios --profile preview",
    "build:ios:prod": "pnpm eas build --platform ios --profile production",
    "build:all": "pnpm eas build --platform all --profile preview",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/query-async-storage-persister": "^5.90.22",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-persist-client": "^5.90.22",
    "expo": "~54.0.31",
    "expo-status-bar": "~3.0.9",
    "react": "^19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "~4.16.0"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@types/react": "^19.2.0",
    "eas-cli": "^16.28.0",
    "typescript": "^5.6.3"
  }
}
